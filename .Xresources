urxvt.scrollBar: false

!Fonts

urxvt.font: xft:Iosevka:pixelsize=14,style=regular
urxvt.boldFont: xft:Iosevka:pixelsize=14,style=bold
urxvt.letterSpace: -1

! special
*.foreground:   #434343
*.background:   #ffffff
*.cursorColor:  #434343

! black
*.color0:       #8c8c8c
