! red
*.color1:       #494949
*.color9:       #4f4f4f

! green
*.color2:       #555555
*.color10:      #5b5b5b

! yellow
*.color3:       #616161
*.color11:      #676767

! blue
*.color4:       #6d6d6d
*.color12:      #727272

! magenta
*.color5:       #787878
*.color13:      #7e7e7e

! cyan
*.color6:       #848484
*.color14:      #8a8a8a

! white
*.color7:       #909090
*.color15:      #969696



